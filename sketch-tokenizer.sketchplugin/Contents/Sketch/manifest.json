{
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "icon.png",
  "commands": [
    {
      "name": "Fill color",
      "identifier": "base-fill-color",
      "shortcut": "ctrl shift w",
      "script": "get-base-fill-color.js"
    },
    {
      "name": "Border color",
      "identifier": "base-border-color",
      "shortcut": "ctrl shift b",
      "script": "get-base-border-color.js"
    },
    {
      "name": "Input",
      "identifier": "token-border-color-input",
      "script": "get-token-border-color-input.js"
    },
    {
      "name": "Input",
      "identifier": "token-background-color-input",
      "script": "get-token-background-color-input.js"
    },
    {
      "name": "Import base colors decisions file...",
      "identifier": "import-base-colors-decisions-file",
      "shortcut": "ctrl shift i",
      "script": "import-base-colors-decisions-file.js"
    },
    {
      "name": "Import borders decisions file...",
      "identifier": "import-token-borders-decisions-file",
      "script": "import-token-borders-decisions-file.js"
    },
    {
      "name": "Import backgrounds decisions file...",
      "identifier": "import-token-backgrounds-decisions-file",
      "script": "import-token-backgrounds-decisions-file.js"
    }
  ],
  "menu": {
    "title": "Tokenizer",
    "items": [
      {
        "title": "Load data",
        "items": [
          "import-base-colors-decisions-file",
          "import-token-borders-decisions-file",
          "import-token-backgrounds-decisions-file"
        ]
      },
      {
        "title": "Identify",
        "items": [
          {
            "title": "Base color",
            "items": [
              "base-fill-color",
              "base-border-color"
            ]
          },
          {
            "title": "Token",
            "items": [
              {
                "title": "Border color",
                "items": [
                  "token-border-color-input"
                ]
              },
              {
                "title": "Background color",
                "items": [
                  "token-background-color-input"
                ]
              }
            ]
          }
        ]
      }
    ]
  },
  "version": "0.1.0",
  "name": "sketch-tokenizer",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com//master/.appcast.xml",
  "author": "lufego",
  "authorEmail": "luis.gomes@commercetools.de"
}