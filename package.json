{
  "name": "sketch-tokenizer",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "sketch-tokenizer",
    "manifest": "src/manifest.json",
    "main": "sketch-tokenizer.sketchplugin",
    "assets": [
